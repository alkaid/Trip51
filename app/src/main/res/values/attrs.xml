<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AppTheme">
        <attr name="actionbarCompatTitleStyle" format="reference" />
        <attr name="actionbarCompatItemStyle" format="reference" />
        <attr name="actionbarCompatItemHomeStyle" format="reference" />
        <attr name="actionbarCompatProgressIndicatorStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="NavigationDot">
        <attr name="navigationDotSelected" format="reference" />
        <attr name="navigationDotUnselected" format="reference" />
        <attr name="navigationDotCount" format="integer" />
    </declare-styleable>

    <declare-styleable name="coodOper">
        <attr name="opMinusBtnBackground" format="reference" />
        <attr name="opAddBtnBackground" format="reference|color" />
        <attr name="opMaxCount" format="integer" />
        <attr name="opMinCount" format="integer" />
        <attr name="numMinWidth" format="dimension|reference" />
        <attr name="iconWidth" format="dimension|reference" />
        <attr name="iconHeight" format="dimension|reference" />
        <attr name="colorStyle" format="enum">
            <enum name="orange" value="0" />
            <enum name="green" value="1" />
        </attr>
    </declare-styleable>
</resources>
